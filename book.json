{
  "title": "HTML講座 補足",
  "description": "Google Map, YouTube, Magnific Popup, Swiper",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "styles": {
    "website": "styles/website.css",
    "pdf": "styles/pdf.css"
  },
  "plugins": [
    "theme-official",
    "page-toc-button",
    "include-codeblock",
    "anchors"
  ],
  "pluginsConfig": {
    "include-codeblock": {
      "template": "./resources/gitbook-plugin-include-codeblock.hbs"
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "image-captions": {
      "caption": "図 _BOOK_IMAGE_NUMBER_. _CAPTION_",
      "align": "left"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontFamily": "VL Gothic",
    "fontSize": 10,
    "paperSize": "a4",
    "margin": {
      "right": 0,
      "left": 0,
      "top": 0,
      "bottom": 0
    }
  }
}
